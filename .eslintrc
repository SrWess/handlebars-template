{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    // "no-console": "error",
    "no-unused-vars": "error",
    "strict": ["error", "global"],
    "curly": "warn"
  }
}
